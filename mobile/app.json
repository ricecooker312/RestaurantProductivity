{
  "expo": {
    "name": "Mealstone",
    "slug": "RestaurantProductivity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/designlogo.png",
    "scheme": "restaurantproductivity",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.saihanr.MealStone",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIDeviceFamily": [1]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/designlogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.saihanr.MealStone"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/designlogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/designlogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "67f603da-5bd3-4eec-a71f-6a5f073b73c5"
      }
    },
    "owner": "saihanr"
  }
}
